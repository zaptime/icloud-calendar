{
    "name": "zaptime/icloud-calendar",
    "description": "",
    "keywords": [
        "zaptime",
        "icloud-calendar"
    ],
    "homepage": "https://github.com/zaptime/icloud-calendar",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Jakub Gause",
            "email": "jakub@zaptime.app",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.2",
        "ext-simplexml": "*",
        "nesbot/carbon": "^2.67",
        "sabre/dav": "^4.6"
    },
    "require-dev": {
        "mockery/mockery": "^1.6",
        "phpstan/phpstan": "^1.10",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Zaptime\\ICloudCalendar\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Zaptime\\ICloudCalendar\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    }
}
